@model IEnumerable<IdentityUser>
@{
    ViewBag.Title = "GetUsers";
}

<h2>Get Users</h2>

<table class="table table-bordered table-striped table-hover js-basic-example dataTable text-center font-weight-bold">
    <thead class="table-light">
        <tr>
            <th>Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.UserName</td>
            <td>
                <a asp-action="Details" asp-controller="Users" asp-route-id="@item.Id"><i style="font-size:large" class="fa fa-eye"></i></a>&nbsp;&nbsp;|&nbsp;&nbsp;
                <a asp-action="Edit" asp-controller="Users" asp-route-id="@item.Id"><i style="font-size:large" class="fa fa-edit"></i></a>&nbsp;&nbsp;|&nbsp;&nbsp;
                <a asp-action="Delete" asp-controller="Users" asp-route-id="@item.Id"><i style="font-size:large" class="fa fa-trash danger"></i></a>
            </td>
           

        </tr>
        }
    </tbody>
</table>