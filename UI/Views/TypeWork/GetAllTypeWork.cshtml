@model DAL.Entities.UserWork.WorkType
@{
    ViewBag.Title = "All Departments";
}
@inject BLL.Services.EmployeePayment.TypeWork.ITypeWorkService Dep
<div class="x_content mt-0">
    <h2 class="p-2 rounded ps-3">All WorkType</h2>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item active" aria-current="page">
                Home
            </li>
            <li class="breadcrumb-item" aria-current="page">
                All Departements
            </li>
        </ol>
    </nav>

</div>
<div class="card-body patients-wating" id="patients-wating">
    <table id="example1" class="table table-striped Actions">
        <thead>
            <tr class="table-bg">
                <th scope="col" class="numbers">#</th>
                <th scope="col">Work Type </th>

              
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            @{int number = 0; }
            @foreach (var item in Dep.GetAll())
            {
                <tr>
                    <th scope="row" class="numbers">
                        <p>@(number+=1)</p>
                    </th>
                    <td>
                        <p class="">@item.Name</p>
                    </td>

                  
                    <td class="w">
                        <div class="row w-75 m-aito">
                            <a class="col-6 btn edite m-0 p-1" data-bs-toggle="tooltip" data-bs-placement="top" title="Edite" asp-action="Edit" asp-controller="TypeWork" asp-route-id="@item.Id">
                                <i class="fa-solid fa-pen-to-square"></i>
                            </a>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<button onclick="history.back()" class="btn btn-secondary"><i class="fas fa-angle-double-left"></i> Back</button>
